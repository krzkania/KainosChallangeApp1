@model List<KainosApp1.Models.CoinsLTC_monthly>
@{
    var myChart = new Chart(width: 1200, height: 800)
        .AddTitle("monthly LTC")

        .AddSeries(
            name: "LITECOIN",
            chartType: "line",
            xValue: Model, xField: "time",
            yValues: Model, yFields: "average")
        .Write();
}